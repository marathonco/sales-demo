{
  "name": "marathon-sales",
  "version": "0.0.1",
  "description": "A Demo/Reservation App",
  "productName": "Marathon Sales App",
  "author": "AwesomeScott",
  "private": true,
  "scripts": {
    "build": "quasar build --mode pwa",
    "dev": "quasar dev --mode spa",
    "dev:pwa": "quasar dev --mode pwa",
    "deploy": "quasar build --mode pwa && firebase deploy --only hosting",
    "deploy:all": "quasar build --mode pwa && firebase deploy",
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "emulator": "firebase emulators:start --import=./emulator --export-on-exit"
  },
  "dependencies": {
    "@quasar/extras": "^1.10.8",
    "core-js": "^3.6.5",
    "date-holidays": "^3.8.0",
    "firebase": "^8.7.1",
